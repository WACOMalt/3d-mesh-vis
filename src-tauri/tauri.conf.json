{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "3d-mesh-vis",
  "version": "1.0.17",
  "identifier": "com.wacomalt.3dmeshvis",
  "build": {
    "frontendDist": "../public",
    "devUrl": "http://localhost:5678",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": null
  },
  "app": {
    "windows": [
      {
        "title": "3D Mesh Visualizer",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
